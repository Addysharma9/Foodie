{
  "expo": {
    "name": "Fozfo",
    "slug": "Fozfo",
    "icon": "./assets/icong.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arunsharma9018.Foodie",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.googleusercontent.apps.88618355277-tqcmbevvio",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.88618355277-tqcmbevvio"
            ]
          }
        ],
        "NSLocationWhenInUseUsageDescription": "This app uses your location to provide better delivery services.",
        "NSCameraUsageDescription": "This app uses the camera for taking profile pictures.",
        "NSPhotoLibraryUsageDescription": "The app accesses your photos for profile pictures.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "212.38.94.189": {
              "NSExceptionAllowsInsecureLoads": true,
              "NSMinimumTLSVersion": "1.0",
              "NSIncludesSubdomains": true
            }
          }
        }
      },
      "infoPlistPrivacy": {
        "NSLocationWhenInUseUsageDescription": "Allow location usage for delivery services"
      }
    },
    "android": {
      "package": "com.arunsharma9018.Foodie",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "permissions": [
        "LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "POST_NOTIFICATIONS"
      ],
      "useNextNotificationsApi": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "com.googleusercontent.apps.88618355277-tqcmbevvio"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.88618355277-tqcmbevvio"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-notifications"
    ],
    "notification": {
      "icon": "./assets/icong.png",
      "color": "#FF6C00",
      "ios": {
        "sound": true,
        "criticalAlert": false,
        "badge": true,
        "alert": true
      },
      "android": {
        "channelId": "meal-reminders",
        "sound": true,
        "priority": "high",
        "vibrate": [
          0,
          250,
          250,
          250
        ],
        "icon": "./assets/icong.png",
        "color": "#FF6C00"
      }
    },
    "extra": {
      "eas": {
        "projectId": "e85a8787-77e4-4d6c-8028-e58067874f31"
      }
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "scheme": "com.googleusercontent.apps.88618355277-tqcmbevvio",
    "owner": "arunsharma9018"
  }
}
